{
  "presets": [
    "react", "es2015", "es2016", "stage-2"
  ],
  "plugins":[
    "transform-flow-strip-types"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    },
    "production": {
      "plugins": [
        "babel-plugin-unassert",
        "babel-plugin-transform-remove-console"
      ]
    }
  }
}
